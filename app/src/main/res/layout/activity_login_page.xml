        android:clipToPadding="false"
        android:id="@+id/rsipjr0ztt3"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#FFFFFF">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/rs1jm9aoz6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="17dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="vertical">
                    <LinearLayout
                        android:id="@+id/r11zemp8nbb7j"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#FCBD02"
                        android:paddingHorizontal="58dp"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/r8enjj0unl2"
                            android:layout_width="304dp"
                            android:layout_height="320dp"
                            android:layout_marginTop="29dp"
                            android:src="@drawable/imageassets_login_signup"
